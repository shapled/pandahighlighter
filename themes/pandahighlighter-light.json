    "tokenColors": [
      {
        "name": "comment",
        "scope": ["comment", "comment.line.double-slash"],
        "settings": {
          "fontStyle": "italic underline",
          "foreground": "#8b8b8b"
        }
      },
      {
        "name": "tags and storage",
        "scope": [
          "entity.name.tag",
          "storage.modifier",
          "storage.type.class",
          "support.type.property-name",
          "punctuation.definition.template-expression",
          "variable.other.readwrite",
          "variable.other.object"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#000000"
        }
      },
      {
        "name": "names",
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#000000"
        }
      },
      {
        "name": "types",
        "scope": [
          "support.type.primitive",
          "entity.name.type",
          "keyword.control.flow"
        ],
        "settings": {
          "fontStyle": "italic underline",
          "foreground": "#000000"
        }
      },
      {
        "name": "functions",
        "scope": ["entity.name.function", "storage.type"],
        "settings": {
          "fontStyle": "italic bold",
          "foreground": "#000000"
        }
      },
      {
        "name": "condifionals and cycles",
        "scope": ["keyword.control.conditional"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#000000"
        }
      },
      {
        "name": "symbols",
        "scope": [
          "keyword.operator.assignment",
          "storage.type.function.arrow",
          "keyword.operator.logical",
          "keyword.operator.comparison",
          "punctuation.separator.key-value",
          "keyword.operator.type.annotation",
          "punctuation.accessor",
          "punctuation.separator.comma",
          "punctuation.definition.string",
          "punctuation.terminator.statement",
          "punctuation.definition.typeparameters"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#000000"
        }
      },
      {
        "name": "gray symbols",
        "scope": [
          "punctuation.separator.array",
          "punctuation.separator.dictionary"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#8b8b8b"
        }
      },
      {
        "name": "exports",
        "scope": ["keyword.control.export", "keyword.control.import"],
        "settings": {
          "fontStyle": "bold italic",
          "foreground": "#000000"
        }
      },
      {
        "name": "quotes",
        "scope": ["string.quoted.single", "string.quoted.double"],
        "settings": {
          "fontStyle": "",
          "foreground": "#525252"
        }
      },
      {
        "name": "properties",
        "scope": ["variable.other.object.property"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#525252"
        }
      }
    ]
  }
  